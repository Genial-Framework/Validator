{
    "name": "genial/validator",
    "type": "library",
    "description": "Validator dependent from Genial Framework",
    "homepage": "https://github.com/Genial-Framework/Validator",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.0",
        "ext-mbstring": "*",
        "genial/di"
    },
    "autoload": {
        "psr-4": {
            "Genial\\Validator\\": "src/"
        },
        "files": ["ar.predefined.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "genial/di"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
