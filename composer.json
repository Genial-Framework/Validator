{
    "name": "genial-framework/validator",
    "type": "library",
    "keywords": [
        "library",
        "php-framework",
        "genial-framework",
        "php-component",
        "validator",
        "php72"
    ],
    "description": "Validator dependent for Genial Framework.",
    "homepage": "https://github.com/Genial-Framework/Validator",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish0820@outlook.com",
            "homepage": "https://github.com/Nenglish7",
            "role": "Develop\Maintain"
        },
        {
            "name": "Genial Contributors",
            "homepage": "https://github.com/orgs/Genial-Framework/people",
            "role": "Contribute"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Genial\\Validator\\": "src/"
        },
        "files": ["main.bootstrap.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
